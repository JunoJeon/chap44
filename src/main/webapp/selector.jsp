<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>selector.jsp</title>
<style type="text/css">
#b1 {
	border: 1px solid red;
	width: 100px;
	height: 100px;
}

#b2 {
	border: 5px dashed blue;
	width: 200px;
	height: 200px;
}

#b3 {
	border: 5px dotted green;
	width: 400px;
	height: 400px;
}

div {
	border-radius: 10px 50px;
}
</style>
</head>
<body>
<div id="b1">box1</div>
<div id="b2">box2</div>
<div id="b3">box3</div>
</body>
</html>