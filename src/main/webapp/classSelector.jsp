<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>classSelector</title>
<style type="text/css">
.small {    
	width : 100px;
	height: 100px;
}
.middle {
	width: 200px;
	height: 200px;
}
.large {
	width: 300px;
	height: 300px;
}

.red {
	background: red;
	border: 10px solid blue;
}

.blue {
	background: blue;
	border: 5px dashed red;
}

</style>
</head>
<body>
<div class = "small red">box1</div>
<div class = "middle">box2</div>
<div class = "large blue">box3</div>
</body>
</html>